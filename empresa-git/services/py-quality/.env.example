# Python Quality Service Configuration

# Server
QUALITY_PORT=8000
QUALITY_HOST=0.0.0.0

# Storage
QUALITY_TMP_DIR=./.quality_tmp

# Phone validation
PHONE_CC=+34
PHONE_LENGTH=9

# Duplicates detection
DUP_THRESHOLD=0.90
